{
  "expo": {
    "name": "Cookout Dash",
    "slug": "cookout-dash",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ff6b35"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cookoutdash.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ff6b35"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.cookoutdash.app",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-av",
      "expo-audio"
    ],
    "extra": {
      "eas": {
        "projectId": "986a1ecf-2ca1-4c64-8dbb-a3881b459306"
      }
    },
    "owner": "defipapi"
  }
}
